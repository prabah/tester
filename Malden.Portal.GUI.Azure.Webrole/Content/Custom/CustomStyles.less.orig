@font-face {
    font-family: HelveticaNeue;
    src: url('fonts/HelveticaNeue.ttf') format('truetype');
    src: url(fonts/helveticaneue-webfont.eot) format('embedded-opentype');
    src: url(fonts/helveticaneue-webfont.woff) format('woff');
}

