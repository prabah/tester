.arrowOldRelease {
    background: url("images/more-arrow.png") no-repeat scroll 0 0 rgba(0, 0, 0, 0);
    display: block;
    height: 20px;
    margin-top: -20px;
    position: absolute;
    right: 8px;
    width: 20px;
}

.downloadRelease {
    position: relative;
    margin: 20px 0px 0px 0px;
    padding: 8px 8px 8px 8px;
    color: #fff;
    font-size: .8em;
    line-height: 18px;
    font-weight: bold;
    -webkit-text-shadow: 1px 1px 1px rgba(0,0,0,0.50);
    margin: 20px 0 16px;
    text-shadow: 1px 1px 1px rgba(0,0,0,0.50);
    background: #ffb42c;
    background: -moz-linear-gradient(top, #ffb42c 0%, #ff8c20 100%);
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ffb42c), color-stop(100%,#ff8c20));
    background: -webkit-linear-gradient(top, #ffb42c 0%,#ff8c20 100%);
    background: -o-linear-gradient(top, #ffb42c 0%,#ff8c20 100%);
    background: -ms-linear-gradient(top, #ffb42c 0%,#ff8c20 100%);
    border: 1px solid #d16d00;
    border-radius: 15px;
    /*min-width: 225px;*/
    display: inline-block;
    width: 120px;
    text-align: center;
}

.archive {
    text-decoration: underline;
}

.downloadReleaseOld1 {
    position: relative;
    margin: 20px 0px 0px 0px;
    padding: 8px 52px 8px 16px;
    color: #fff;
    font-size: .8em;
    line-height: 18px;
    font-weight: bold;
    -webkit-text-shadow: 1px 1px 1px rgba(0,0,0,0.50);
