@using Malden.Portal.GUI.Azure.Webrole.Utilities

@model Malden.Portal.BLL.UserPurchase
@*<script src="~/Scripts/jquery-2.1.0.min.js"></script>*@

@*<script src='<%# ResolveUrl("~/Scripts/jquery-2.1.0.min.js")%>'></script>*@
<script src="~/Scripts/jquery-2.1.1.min.js"></script>

<span id="user"></span>
@using (Html.BeginForm("Create", "Home", FormMethod.Post, new { defaultButton = "addItem"}))
{
    @Html.AntiForgeryToken()
    @Html.ValidationSummary(true)

    <div style="font-size: 0.875em;">
        @Html.Raw(CustomHelpers.FindSerialNumberString(""))
    </div>

    <div class="editor-field">
        <table>
            <tr>
                <td width="140" valign="middle">@Html.EditorFor(model => model.RegistrationCode)</td>
                <td valign="middle"><input type="submit" id="addItem" value="Register" class="malden-button" style="font-size: 70%;" /></td>
            </tr>
            <tr>
                <td colspan="2">@Html.ValidationMessageFor(model => model.RegistrationCode)</td>
            </tr>
        </table>
    </div>
    
    <p>
    </p>
}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")

    @*<script src="~/Scripts/Custom/custom-functions.js"></script>*@
}
