// Variable declarations - start
@labelLength: 125px;
@marginRight: 50px;
@contentWidth: 800px;
@rightAlignedMargin: 50px;
@inputBoxWidth: (@contentWidth * 60/100);
@paddingAndMargin_Body: 15px;

@totalUsedSpace: @labelLength + @marginRight + @rightAlignedMargin + @inputBoxWidth + @paddingAndMargin_Body;


@calculatedRightAlignedMargin: 330px;

@backgroundColor: #258cef;
// Variable declarations - end

//1.0 ---> Custom Styles for page elements
//2.0 ---> Validation error messages

//1.0
body, html {
    margin: 0;
    font-family: Arial, 'DejaVu Sans', 'Liberation Sans', Freesans, sans-serif;
    font-size: 100%;
}


.active {
    color: #000;
    text-decoration: underline;
    background-color: aliceblue;

}


.login {
    margin-left: auto;
    margin-right: auto;
    vertical-align: central;
}


.right-aligned {
    padding-top: 15px;
    text-align: right;
    margin-right: @rightAlignedMargin; //@calculatedRightAlignedMargin;
}

    .right-aligned a {
        margin-right: 8px;
        color: @backgroundColor;
        font-size: 1.125em;
        font-weight: bold;
        font-family: Arial,sans-serif;
    }



a {
    text-decoration: none;
    margin: 0;
    font-family: Arial,sans-serif;
}

.editor-label {
    padding-bottom: 5px;
}

#ProductDetails {
    padding-top: 20px;
    padding-bottom: 20px;
}

#RegistrationCode {
    width: 200px;
}

