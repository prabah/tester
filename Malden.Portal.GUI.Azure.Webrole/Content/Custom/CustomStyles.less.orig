}

a.releasenotes {
    position: relative;
    margin: 20px 0px 0px 0px;
    /*padding: 8px 52px 8px 16px;*/
    padding: 8px;
    display: inline-block;
    width: 120px;
    color: #fff;
    font-size: .8em;
    line-height: 18px;
    margin: 20px 0 16px;
    font-weight: bold;
    -webkit-text-shadow: 1px 1px 1px rgba(0,0,0,0.50);
    text-shadow: 1px 1px 1px rgba(0,0,0,0.50);
    background: #7BC22A; /* Old browsers */
    background: -moz-linear-gradient(top, #7BC22A 0%, #5FA812 100%); /* FF3.6+ */
    background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#7BC22A), color-stop(100%,#5FA812)); /* Chrome,Safari4+ */
    background: -webkit-linear-gradient(top, #7BC22A 0%,#5FA812 100%); /* Chrome10+,Safari5.1+ */
    background: -o-linear-gradient(top, #7BC22A 0%,#5FA812 100%); /* Opera11.10+ */
    background: -ms-linear-gradient(top, #7BC22A 0%,#5FA812 100%); /* IE10+ */
    border: 1px solid #58a007;
    -moz-border-radius: 15px;
    -ms-border-radius: 15px;
    -o-border-radius: 15px;
    -webkit-border-radius: 15px;
    border-radius: 15px;
}

    a.releasenotes span.arrowOldRelease {
        position: absolute;
        width: 20px;
        height: 20px;
        display: block;
        background: url("images/note-arrow.png") no-repeat scroll 0px 0px transparent;
    }

