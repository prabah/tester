fieldset {
    border: 0;
    width: @contentWidth;
    min-height: 600px;
    margin-left: -9px;
}

legend {
    padding: 0.2em 0.5em;
    border: none;
    font-size: 90%;
    text-align: right;
}


ol li {
    text-align: left;
}

    ol li label {
        width: @labelLength;
        display: inline-block;
        font-family: MaldenUI;
        font-size: 1.2em;
        font-weight: 600;
        margin-right: @marginRight;
    }

    ol li input[type="text"] {
        margin-right: 0;
        display: inline-block;
    }

    ol li input[type="submit"] {
        position: relative;
        margin: 20px 0px 0px 0px;
        padding: 8px 52px 8px 16px;
        color: #fff;
        
        line-height: 18px;
        font-weight: bold;
        -webkit-text-shadow: 1px 1px 1px rgba(0,0,0,0.50);
        text-shadow: 1px 1px 1px rgba(0,0,0,0.50);
        background: #ffb42c;
        background: -moz-linear-gradient(top, #ffb42c 0%, #ff8c20 100%);
        background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ffb42c), color-stop(100%,#ff8c20));
        background: -webkit-linear-gradient(top, #ffb42c 0%,#ff8c20 100%);
        background: -o-linear-gradient(top, #ffb42c 0%,#ff8c20 100%);
        background: -ms-linear-gradient(top, #ffb42c 0%,#ff8c20 100%);
        border: 1px solid #d16d00;
        border-radius: 15px;
        width: 125px;
        /*padding: 7px;*/
        margin-right: 8px;
    }

.arrow {
    position: absolute;
    margin-top: -23px;
    right: @calculatedRightAlignedMargin;
    width: 20px;
    height: 20px;
    display: block;
    background: url(images/more-arrow.png) no-repeat 0 0;
}

label.checkbox {
    display: inline;
}

.login-submit {
    padding-top: 5px;
    line-height: 100px;
}

input, textarea, select {
    border: 1px solid #e2e2e2;
    background: #fff;
    color: #333;
    font-size: 1.2em;
    margin: 5px 0 6px 0;
    padding: 5px;
    width: @inputBoxWidth;
}

textarea {
    font-family: inherit;
}

    input:focus, textarea:focus {
        border: 1px solid #7ac0da;
    }

input[type="checkbox"] {
    background: transparent;
    border: inherit;
    width: auto;
}

input[type="submit"],
input[type="button"],
button {
    background-color: #d3dce0;
    border: 1px solid #787878;
    cursor: pointer;
    font-size: 1.2em;
    font-weight: 600;
    padding: 7px;
    margin-right: 8px;
    width: auto;
}

html > body input[type="submit"] {
    padding: 4px;
    width: 125px;
    border-radius: 15px;
}


//2.0
.error {
    color: #e80c4d;
}

/* styles for validation helpers */
.field-validation-error {
    color: #e80c4d;
    font-weight: bold;
}

.field-validation-valid {
    display: none;
}

input.input-validation-error {
    border: 1px solid #e80c4d;
}

input[type="checkbox"].input-validation-error {
    border: 0 none;
}

.validation-summary-errors {
    color: #e80c4d;
    font-weight: bold;
    font-size: 1.1em;
}

.validation-summary-valid {
    display: none;
}

